      "source": [
        "# Create a list with 7 elements\n",
        "counters = [0] * 7\n",
        "\n",
        "# Sample input â€”\n",
        "tosses = [1, 2, 3, 4, 5, 6]\n",
        "\n",
        "# Count each die toss\n",
        "for value in tosses:\n",
        "    if 1 <= value <= 6:\n",
        "        counters[value] += 1\n",
        "\n",
        "# Print the results\n",
        "for i in range(1, 7):\n",
        "    print(f\"{i}: {counters[i]}\")\n"
      ]
